defutf8 on
defencoding utf8
encoding utf-8 utf-8

escape ^t^t
startup_message off
autodetach on
vbell off
caption always "%{= gk}%-w%10L>%{=b kg}%n %t%{-}%+w%-0="
hardstatus alwayslastline "%H%=[%Y/%m/%d %02c]"
shelltitle "$ |shell"
termcapinfo xterm* ti@:te@
shell $SHELL
defscrollback 5000
altscreen on
