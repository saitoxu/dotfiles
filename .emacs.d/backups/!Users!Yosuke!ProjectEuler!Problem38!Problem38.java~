import java.io.*;

public class Problem38 {
    public static void main(String[] args) {
        int maxPandigital = 918273645;
        int tmp = 0;
        for (int i = 90; i < 100; i++) {
            tmp = calculateConcatenatedProduct(i);
            if (tmp > maxPandigital) {
                maxPandigital = tmp;
            }
        }

        for (int i = 900; i < 1000; i++) {
            tmp = calculateConcatenatedProduct(i);
            if (tmp > maxPandigital) {
                maxPandigital = tmp;
            }
        }

        for (int i = 9000; i < 10000; i++) {
            tmp = calculateConcatenatedProduct(i);
            if (tmp > maxPandigital) {
                maxPandigital = tmp;
            }
        }

        System.out.println("answer = " + maxPandigital);
    }

    // 9桁かつpandigitalのとき以外は0を返す
    private static int calculateConcatenatedProduct(int i) {
        StringBuffer buf = new StringBuffer();
        int num = 1;

        while (buf.length() < 9) {
            buf.append(Integer.toString(i * num));
            num++;
        }
        // System.out.println(buf.toString());

        if (buf.length() == 9 && isPandigital(buf.toString())) {
            System.out.println(buf.toString());
            return Integer.parseInt(buf.toString());
        } else {
            return 0;
        }
    }

    private static boolean isPandigital(String pandigitalStr) {
        char[] numChars = pandigitalStr.toCharArray();

        for (int i = 0; i < numChars.length; i++) {
            for (int j = i + 1; j < numChars.length; j++) {
                // 0含んでたらfalse
                if (numChars[i] == '0' || numChars[i] == numChars[j]) {
                    return false;
                }
            }
        }

        return true;
    }
}
