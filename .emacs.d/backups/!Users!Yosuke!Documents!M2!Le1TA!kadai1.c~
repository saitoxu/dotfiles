#include <stdio.h>
#include <stdlib.h>

int main(void)
{
  FILE *fp1;
  char s[256];
  if ((fp1 = fopen("mylecture.txt", "r")) == NULL) {
    printf("file open error!!\n");
    exit(EXIT_FAILURE);
  }
  FILE *fp2;
  if ((fp2 = fopen("mylecture.data", "w")) == NULL) {
    printf("file open error!!\n");
    exit(EXIT_FAILURE);
  }
  while (fgets(s, 256, fp1) != NULL) {

    fprintf(fp2,"Mo1\t%s",s);
  }
  fclose(fp1);
  fclose(fp2);
  return(0);
}
